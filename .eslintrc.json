{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "no-restricted-syntax": "warn",
        "no-underscore-dangle": "warn",
        "class-methods-use-this": "off",
        "max-len": [2, 120],
        "no-console": "off"
    }
}
